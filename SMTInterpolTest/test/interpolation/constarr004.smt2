(set-option :produce-proofs true)
(set-option :proof-check-mode true)
(set-option :interpolant-check-mode true)
(set-option :print-terms-cse false)

(set-logic QF_AUFLIA)
(declare-sort U 0)
(declare-fun v () U)
(declare-fun w1 () U)
(declare-fun w2 () U)
(declare-fun w3 () U)
(declare-fun i () U)
(declare-fun k1 () U)
(declare-fun k2 () U)
(declare-fun k3 () U)
(declare-fun p (U) Bool)
(declare-fun p2 (U) Bool)
(declare-fun p3 (U) Bool)
(declare-fun a () (Array U U))
(declare-fun s () (Array U U))

(assert (! (and (p (select a i)) (= s (store a k1 w1))
(not (= i k1)) (p2 i) (p3 i)) :named A))
(assert (! (and (not (p v)) (= s (store (store ((as const (Array U U)) v) k2 w2) k3 w3))
(not (p2 k2)) (not (p3 k3))) :named B))

(check-sat)
(get-proof)
(get-interpolants A B)
(exit)
