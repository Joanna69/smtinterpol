(set-option :print-success false)
(set-option :produce-proofs false)
(set-option :verbosity 3)
(set-logic UFLIA)
(set-info :source |Simple quantified example that should be rejected by the Quantifier Theory.|)
(set-info :smt-lib-version 2.0)
(set-info :category "crafted")
(set-info :status unsat)
(declare-fun l () Int)
(declare-fun u () Int)
(declare-fun a (Int) Int)
(assert (< l u))
(assert (forall ((x Int)) (=> (and (<= l x) (< x u)) (<= (a x) (a (+ x 1))))))
(assert (> (a (+ l 1)) (a u)))
(check-sat)
(exit)
