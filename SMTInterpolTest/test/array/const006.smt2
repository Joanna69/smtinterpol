(set-option :produce-proofs true)
(set-option :proof-check-mode true)
(set-option :print-terms-cse false)

(set-logic QF_AX)
(declare-sort U 0)
(declare-fun v () U)
(declare-fun i1 () U)
(declare-fun i2() U)
(declare-fun v1 () U)
(declare-fun v2 () U)
(declare-fun a () (Array U U))
(define-fun constU ((v U)) (Array U U) ((as const (Array U U)) v))

(assert (= a (store (store ((as const (Array U U)) v) i1 v1) i2 v2)))
(assert (= (select a i1) v))
(assert (= (select a i2) v))
(assert (not (= a ((as const (Array U U)) v))))
(check-sat)
(get-proof)
(exit)