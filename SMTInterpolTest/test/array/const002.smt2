(set-option :produce-proofs true)
(set-option :proof-check-mode true)

(set-logic QF_AX)
(declare-sort U 0)
(declare-fun v1 () U)
(declare-fun v2 () U)
(declare-fun i () U)
(declare-fun w () U)
(declare-fun a () (Array U U))
(define-fun constU ((v U)) (Array U U) ((as const (Array U U)) v))

(assert (= a (store ((as const (Array U U)) v1) i w)))
(assert (= a (store ((as const (Array U U)) v2) i w)))
(assert (not (= v1 v2)))
(check-sat)
(get-proof)
(exit)